<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.coursework.coursework" >



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >


        <activity
            android:name=".SingleFragmentActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ListOrderActivity"
            android:label="@string/app_name"
            android:parentActivityName=".LogInActivity">
        </activity>
        <activity
            android:name=".MakeOrderActivity"
            android:label="@string/app_name"
            android:parentActivityName=".ListOrderActivity">
        </activity>

        <activity
            android:name=".RegestrationActivity"
            android:label="@string/app_name"
            android:parentActivityName=".LogInActivity">
        </activity>


        <activity
            android:name=".LogInActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
